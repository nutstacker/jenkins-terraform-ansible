pipeline{

    environment {
        PATH = "/opt/apache-maven-3.8.1/bin:$PATH"
    }

    agent any
    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/nutstacker/hello-world-war'
            }
        }  
        stage('Maven Build') {
            steps {
                sh 'mvn clean install'
            }
        } 
    }
}
