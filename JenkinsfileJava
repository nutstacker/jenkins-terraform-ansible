pipeline{

    environment {
        AWS_ACCESS_KEY_ID     = credentials('AWS_ACCESS_KEY')
        AWS_SECRET_ACCESS_KEY = credentials('AWS_SECRET_KEY')
    }

    agent any
    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/nutstacker/hello-world-war'
            }
        }  
        stage('checkout') {
            steps {
                sh 'mvn clean install'
            }
        } 
        }
    }
}
